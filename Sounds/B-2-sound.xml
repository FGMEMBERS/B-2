<?xml version="1.0"?>

<PropertyList>
  
<fx>


<!-- engine0 -->
<engine>
	<name>engine0</name>
	<path>Sounds/jet.wav</path>
	<mode>looped</mode>
	<property>/engines/engine[0]/running</property>

	<orientation>
		<x>-9.0</x>
		<y>0.0</y>
		<z>180.0</z>
		<inner-cone>180.0</inner-cone>
		<outer-cone>360.0</outer-cone>
		<outer-gain>0.2</outer-gain>
	</orientation>
	<volume>
		<property>/engines/engine[0]/thrust_lb</property>
		<factor>0.000019000</factor>
	</volume>
</engine>



<engine>
	<name>external whine0</name>
	<path>Sounds/turbojet_fan.wav</path>
	<mode>looped</mode>
	<condition>
		<not>
			<property>/sim/current-view/internal</property>
		</not>
		<property>/engines/engine[0]/running</property>
	</condition>

	<orientation>
		<x>-9.0</x>
		<y>-5.0</y>
		<z>0.0</z>
		<inner-cone>50.0</inner-cone>
		<outer-cone>180.0</outer-cone>
		<outer-gain>0.0</outer-gain>
	</orientation>
	<pitch>
		<property>/engines/engine[0]/n1</property>
		<ofset>0.7</ofset>
		<factor>.003</factor>
	</pitch>
	<volume>
		<property>/engines/engine[0]/n1</property>
		<offset>1.0</offset>
		<factor>-0.008</factor>
		<max>0.6</max>
	</volume>
</engine>

<engine>
	<name>internal whine0</name>
	<path>Sounds/whine.wav</path>
	<mode>looped</mode>
	<condition>
		<property>/sim/current-view/internal</property>
		<property>/engines/engine[0]/running</property>
	</condition>

	<orientation>
		<x>-9.0</x>
		<y>-12.0</y>
		<z>0.0</z>
		<inner-cone>30.0</inner-cone>
		<outer-cone>120.0</outer-cone>
		<outer-gain>0.0</outer-gain>
	</orientation>
	<pitch>
		<property>/engines/engine[0]/n1</property>
		<ofset>0.7</ofset>
		<factor>.00075</factor>
	</pitch>
	<volume>
		<max>0.6</max>
	</volume>
  </engine>
  
