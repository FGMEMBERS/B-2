<PropertyList>

 <path>tcb2.ac</path>

 
<!-- sideslip anim  xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx -->

<animation>
	<type>translate</type>
	<object-name>SlipBall</object-name>
	<property>/instrumentation/slip-skid-ball/indicated-slip-skid</property>
	<min>-1.1</min>
	<max>1.1</max>
	<interpolation>
		<entry>
			<ind>-1.1</ind>
			<dep>-0.0125</dep>
		</entry>
		<entry>
			<ind>1.1</ind>
			<dep>0.0125</dep>
		</entry>
	</interpolation>
	<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
	</axis>
 </animation>


</PropertyList>